<%= @gym["name"] %> <br>
<%= @gym["address"] %> <br>
<%= @gym["bio"] %> <br>
<%= @gym["fee"] %> <br>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Client</th>
    </tr>
  </thead>
  <tbody>
    <% @workouts.each do |workout| %>
      <tr>
        <td><%= workout["date"] %></td>
        <td><%= workout["time"] %></td>
        <td><%=workout.agreement.client["name"]%></td>
      </tr>
    <% end %>
  </tbody>
</table>
